#################################################################################
# palette
#################################################################################
/def /purewhite actor:s
     /color/add $actor 1 1 1

/def /white actor:s
     /color/add $actor 0.97 0.95 0.87

/def /black actor:s
     /color/add $actor 0 0 0

/def /blue/dk actor:s
     /color/add $actor 0.05 0.08 0.09

/def /blue actor:s
     /color/add $actor 0.21 0.28 0.32

/def /blue/mid actor:s
    /color/add $actor 0.35 0.4 0.45

/def /blue/lt actor:s
    /color/add $actor 0.45 0.58 0.66

/def /yellow actor:s
     /color/add $actor 1 0.93 0.29

/def /orange actor:s
     /color/add $actor 0.91 0.43 0.09

/def /orange/abstract actor:s
     /color/add $actor 0.91 0.63 0.2

/def /orange/lt actor:s
