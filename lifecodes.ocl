/def /red actor:s
     /color/add $actor 0.32 0.05 0.08

/def /red/lt actor:s
     /color/add $actor 0.42 0.05 0.08

/def /red/dk actor:s
     /color/add $actor 0.32 0.05 0.08

/def /gray/dk actor:s
